# Smart Contract Deployment Configuration
# Copy this file to .env.deployment and fill in your values

# Network Configuration
# Options: sepolia, mainnet, localhost
NETWORK=sepolia

# Private Key for Deployment
# WARNING: Never commit this file to version control
PRIVATE_KEY=your_private_key_here

# TEE Signer Address (optional, defaults to deployer address)
TEE_SIGNER_ADDRESS=0x3ED0B957Fd306FEB580A7dAe191ce71BA4157B48

# RPC URLs
SEPOLIA_RPC_URL=https://rpc.sepolia.org
MAINNET_RPC_URL=https://eth.llamarpc.com

# Etherscan API Key (for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas Configuration (optional)
# If not set, network gas price will be used
GAS_PRICE=20
GAS_LIMIT=6000000

# Auto Verification
# Set to 'true' to automatically verify contracts on Etherscan
AUTO_VERIFY=false
